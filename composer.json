{
  "name": "survos/thumb-hash-bundle",
  "type": "symfony-bundle",
  "description": "Symfony bundle for srwiez/thumbhash PHP library",
  "keywords": ["symfony-ux", "symfony"],

  "license": "MIT",
  "require": {
    "php": "^8.3",
    "symfony/config": "^6.4 || ^7.1",
    "symfony/dependency-injection": "^6.4 || ^7.1",
    "symfony/http-kernel": "^6.4 || ^7.1",
    "twig/twig": "^3.4",
    "srwiez/thumbhash": "dev-tac",
    "ext-imagick": "*",
    "ext-gd": "*"
  },
  "autoload": {
    "psr-4": {
      "Survos\\ThumbHashBundle\\": "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "symfony/browser-kit": "^6.4 || ^7.1",
    "symfony/framework-bundle": "^6.4 || ^7.1",
    "symfony/phpunit-bridge": "^6.4 || ^7.1",
    "symfony/twig-bundle": "^6.4 || ^7.1",
    "symfony/var-dumper": "^6.4 || ^7.1"
  },
  "repositories": {
    "thumbhash": {
      "type": "vcs",
      "url": "git@github.com:tacman/thumbhash.git"
    }
  }
}
